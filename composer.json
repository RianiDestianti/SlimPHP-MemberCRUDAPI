{
    "name": "nanakam/slim",
    "description": "Slim 4 Skeleton",
    "require": {
        "php": "^7.4",
        "aws/aws-sdk-php": "3.164.0",
        "envms/fluentpdo": "^2.2.0",
        "illuminate/database": "^8.83",
        "lcobucci/jwt": "^4.0",
        "mpdf/mpdf": "^8.1",
        "phpoffice/phpspreadsheet": "^1.25",
        "pimple/pimple": "^3.2",
        "predis/predis": "^1.1",
        "respect/validation": "^2.2",
        "selective/basepath": "^2.1",
        "sendgrid/sendgrid": "~7",
        "slim/http-cache": "^1.1",
        "slim/psr7": "^1.0.0",
        "slim/slim": "^4.8",
        "slim/twig-view": "^3.2",
        "symfony/cache": "^5.3",
        "vlucas/phpdotenv": "^5.1"
    },
    "config": {
        "platform": {
            "php": "7.4.11"
        },
        "sort-packages": true,
        "process-timeout": 0
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "serve": [
            "cd public && php -S localhost:8000"
        ]
    }
}
